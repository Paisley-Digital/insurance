{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": [
      "es2020",
      "dom"
    ],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@./assets": ["libs/shared/assets/src/index.ts"],
      "@./locales": ["libs/shared/util/locales/src/index.ts"],
      "@insurance-employee-feature-upload": ["libs/employee/feature/upload/src/index.ts"],
      "@./assets": [
        "libs/shared/assets/src/index.ts"
      ],
      "@insurance-employee-data-dashboards": [
        "libs/employee/data/data-dashboards/src/index.ts"
      ],
      "@./locales": [
        "libs/shared/util/locales/src/index.ts"
      ],
      "@insurance-broker-feature-auth": [
        "libs/broker/feature/auth/src/index.ts"
      ],
      "@insurance-employee-feature-auth": [
        "libs/employee/feature/auth/src/index.ts"
      ],
      "@insurance-employee-feature-dashboard": [
        "libs/employee/feature/dashboard/src/index.ts"
      ],
      "@insurance-employee-feature-shell": [
        "libs/employee/feature/shell/src/index.ts"
      ],
      "@insurance-shared-app-core": ["libs/shared/util/appp-core/src/index.ts"],
      "@insurance-shared-app-core": [
        "libs/shared/util/appp-core/src/index.ts"
      ],
      "@insurance-shared-ui-overlay-spinner": [
        "libs/shared/ui/overlay-spinner/src/index.ts"
      ],
      "@shared-ui-header-desktop": [
        "libs/shared/ui/header-desktop/src/index.ts"
      ],
      "@shared-ui-icon": [
        "libs/employee/shared/ui/icon/src/index.ts"
      ],
      "@shared-ui-input-validator": [
        "libs/shared/ui/input-validation-message/src/index.ts"
      ],
      "@shared-util-common": [
        "libs/shared/util/common/src/index.ts"
      ],
      "@shared-util-web-sdk": [
        "libs/shared/util/web-sdk/src/index.ts"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}
